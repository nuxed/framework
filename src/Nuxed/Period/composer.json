{
    "name": "nuxed/period",
    "type": "library",
    "description": "The Nuxed Period Component.",
    "keywords": [
        "hack",
        "hhvm",
        "nuxed",
        "period"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "azjezz",
            "email": "azjezz@protonmail.com",
            "homepage": "https://azjezz.github.io"
        },
        {
            "name": "Nuxed Community",
            "homepage": "https://github.com/nuxed/framework/graphs/contributors"
        }
    ],
    "require": {
        "hhvm": "^4.0",
        "hhvm/hsl": "^4.0",
        "hhvm/hhvm-autoload": "^2.0",
        "nuxed/util": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nuxed\\Period\\": "./"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "type-check": "hh_client ./",
        "check": [
            "@type-check"
        ]
    },
    "support": {
        "issues": "https://github.com/nuxed/framework/issues",
        "chat": "https://gitter.im/nuxed/period",
        "source": "https://github.com/nuxed/period"
    }
}
