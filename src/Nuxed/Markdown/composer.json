{
    "name": "nuxed/markdown",
    "type": "library",
    "description": "The Nuxed Markdown Component.",
    "keywords": [
        "hack",
        "hhvm",
        "nuxed",
        "markdown"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "azjezz",
            "email": "azjezz@protonmail.com",
            "homepage": "https://azjezz.github.io"
        },
        {
            "name": "Nuxed Community",
            "homepage": "https://github.com/nuxed/framework/graphs/contributors"
        }
    ],
    "require": {
        "hhvm": "^4.0",
        "hhvm/hsl": "^4.0",
        "hhvm/hhvm-autoload": "^2.0",
        "facebook/fbmarkdown": "^1.5",
        "facebook/xhp-lib": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Nuxed\\Markdown\\": "./"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": "hhast-lint -v ./",
        "type-check": "hh_client ./",
        "check": [
            "@type-check",
            "@lint"
        ]
    },
    "support": {
        "issues": "https://github.com/nuxed/framework/issues",
        "chat": "https://gitter.im/nuxed/markdown",
        "source": "https://github.com/nuxed/markdown"
    }
}
