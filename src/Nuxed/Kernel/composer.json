{
    "name": "nuxed/kernel",
    "type": "library",
    "description": "The Nuxed Kernel Component.",
    "keywords": [
        "hack",
        "hhvm",
        "nuxed",
        "kernel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "azjezz",
            "email": "azjezz@protonmail.com",
            "homepage": "https://azjezz.github.io"
        },
        {
            "name": "Nuxed Community",
            "homepage": "https://github.com/nuxed/framework/graphs/contributors"
        }
    ],
    "require": {
        "hhvm": "^4.0",
        "hhvm/hsl": "^4.0",
        "hhvm/hhvm-autoload": "^2.0",
        "nuxed/contract": "^0.1.0",
        "nuxed/container": "^0.1.0",
        "nuxed/http": "^0.1.0",
        "nuxed/event": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Nuxed\\Kernel\\": "./"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "type-check": "hh_client ./",
        "check": [
            "@type-check"
        ]
    },
    "support": {
        "issues": "https://github.com/nuxed/framework/issues",
        "chat": "https://gitter.im/nuxed/kernel",
        "source": "https://github.com/nuxed/kernel"
    }
}
